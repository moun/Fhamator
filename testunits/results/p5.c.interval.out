                                      // y:[-oo,+oo]  x:[-oo,+oo]  
if x < 0 {
                                      // y:[-oo,+oo]  x:[-oo,0]  
  while x < 0 {
                                      // y:[-oo,+oo]  x:[-oo,-1]  
    x = x + 1
  };
                                      // y:[-oo,+oo]  x:[0,0]  
  y = x
} else {
                                      // y:[-oo,+oo]  x:[0,+oo]  
  y = 0
}
